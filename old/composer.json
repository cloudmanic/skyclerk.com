{
  "require": {
    "craftcms/aws-s3": "1.2.5",
    "craftcms/cms": "3.3.5",
    "craftcms/contact-form": "2.2.5",
    "craftcms/mailgun": "1.4.2",
    "craftcms/redactor": "2.4.0",
    "dolphiq/sitemap": "1.0.9",
    "doublesecretagency/craft-cpjs": "2.2.0",
    "ether/seo": "3.6.2",
    "jaybizzle/crawler-detect": "^1.2",
    "mmikkel/cp-field-inspect": "1.0.6",
    "topshelfcraft/wordsmith": "3.1.1",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
